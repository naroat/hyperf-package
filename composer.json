    "description": "Toolkit for hyperf",
    "keywords": ["php", "hyperf", "util", "package", "kit"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Naroat\\HyperfPackage\\": "src/"
        },
        "files": [
            "src/Helpers/Function.php"
        ]
